<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>
<web-app>
  <display-name>WebApp1</display-name>
  <servlet>
    <servlet-name>LoginInitParamServlet</servlet-name>
    <display-name>LoginInitParamServlet</display-name>
    <servlet-class>LoginInitParamServlet</servlet-class>
    <init-param>
      <param-name>UserID</param-name>
      <param-value>Anonymous</param-value>
    </init-param>
    <init-param>
      <param-name>Password</param-name>
      <param-value>East123</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginInitParamServlet</servlet-name>
    <url-pattern>/LoginInitParamServletAlias</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>
