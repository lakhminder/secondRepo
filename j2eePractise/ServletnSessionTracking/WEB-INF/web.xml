<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>
<web-app>
  <display-name>WebApp1</display-name>
  <servlet>
    <servlet-name>ResultCalculationServlet</servlet-name>
    <display-name>ResultCalculationServlet</display-name>
    <servlet-class>ResultCalculationServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MarksEntryServlet</servlet-name>
    <display-name>MarksEntryServlet</display-name>
    <servlet-class>MarksEntryServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginValidationServlet</servlet-name>
    <display-name>LoginValidationServlet</display-name>
    <servlet-class>LoginValidationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResultCalculationServlet</servlet-name>
    <url-pattern>/ResultCalculationServletAlias</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MarksEntryServlet</servlet-name>
    <url-pattern>/MarksEntryServletAlias</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginValidationServlet</servlet-name>
    <url-pattern>/LoginValidationServletAlias</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>