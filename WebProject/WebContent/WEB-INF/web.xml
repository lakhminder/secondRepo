<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>WebProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>A simple HTTP Servlet</description>
    <display-name>SimpleServletName</display-name>
    <servlet-name>SimpleServletName</servlet-name>
    <servlet-class>org.lucky.webProject.example1.SimpleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SimpleServletName</servlet-name>
    <url-pattern>/SimpleServletPath</url-pattern>
  </servlet-mapping>
    
  <servlet>
    <description>A Init JSP</description>
    <display-name>InitJSSSPSP!!</display-name>
    <servlet-name>InitJSP</servlet-name>
    <jsp-file>/initJSP.jsp</jsp-file>
    <init-param>
    	<param-name>defaultUser</param-name>
    	<param-value>User Name Tested123</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InitJSP</servlet-name>
  	<url-pattern>/initJSP.jsp</url-pattern>
  </servlet-mapping>
 
</web-app>
